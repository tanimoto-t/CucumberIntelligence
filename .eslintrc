{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "prefer-const": 0,
        "arrow-body-style": 0,
        "no-underscore-dangle": [2, { "allowAfterThis": true } ],
        "no-restricted-syntax": [2, "ForInStatement", "LabeledStatement", "WithStatement" ]
    }
}
